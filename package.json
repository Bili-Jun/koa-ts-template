{
  "name": "koa-ts-template2",
  "version": "0.0.1",
  "description": "A typescript template of koa2",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=koa* NODE_ENV=development nodemon --watch 'src/**/*' -e ts --exec 'npm run lint && npm run ts-node -- --inspect --experimental-modules'",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "ts-node": "ts-node ./src/app.ts",
    "tsc": "node_modules/typescript/bin/tsc && tsc-alias",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "server",
    "koa",
    "ts"
  ],
  "author": "bili-jun",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.4",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-connect": "^2.1.0",
    "koa-convert": "^2.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-onerror": "^4.2.0",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa-views": "^7.0.2",
    "log4js": "^6.7.1",
    "vite": "^2.9.15"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-json": "^2.0.20",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa-views": "^7.0.0",
    "@types/node": "^16.18.10",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
